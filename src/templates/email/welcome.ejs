<table
  class="body-wrap"
  style="
    font-family: 'Roboto', sans-serif;
    box-sizing: border-box;
    font-size: 14px;
    width: 100%;
    background-color: transparent;
    margin: 0;
  "
>
  <tbody>
    <tr
      style="
        font-family: 'Roboto', sans-serif;
        box-sizing: border-box;
        font-size: 14px;
        margin: 0;
      "
    >
      <td
        style="
          font-family: 'Roboto', sans-serif;
          box-sizing: border-box;
          font-size: 14px;
          vertical-align: top;
          margin: 0;
        "
        valign="top"
      ></td>
      <td
        class="container"
        width="800"
        style="
          font-family: 'Roboto', sans-serif;
          box-sizing: border-box;
          font-size: 14px;
          vertical-align: top;
          display: block !important;
          max-width: 800px !important;
          clear: both !important;
          margin: 0 auto;
        "
        valign="top"
      >
        <div
          class="content"
          style="
            font-family: 'Roboto', sans-serif;
            box-sizing: border-box;
            font-size: 14px;
            max-width: 800px;
            display: block;
            margin: 0 auto;
            padding: 20px;
          "
        >
          <table
            class="main"
            width="100%"
            cellpadding="0"
            cellspacing="0"
            itemprop="action"
            itemscope=""
            itemtype="http://schema.org/ConfirmAction"
            style="
              font-family: 'Roboto', sans-serif;
              box-sizing: border-box;
              font-size: 14px;
              border-radius: 3px;
              margin: 0;
              border: none;
            "
          >
            <tbody>
              <tr
                style="
                  font-family: 'Roboto', sans-serif;
                  font-size: 14px;
                  margin: 0;
                "
              >
                <td
                  class="content-wrap"
                  style="
                    font-family: 'Roboto', sans-serif;
                    box-sizing: border-box;
                    color: #495057;
                    font-size: 14px;
                    vertical-align: top;
                    margin: 0;
                    padding: 30px;
                    box-shadow: 0 3px 15px rgba(30, 32, 37, 0.06);
                    border-radius: 7px;
                    background-color: #fff;
                  "
                  valign="top"
                >
                  <meta
                    itemprop="name"
                    content="Confirm Email"
                    style="
                      font-family: 'Roboto', sans-serif;
                      box-sizing: border-box;
                      font-size: 14px;
                      margin: 0;
                    "
                  />
                  <table
                    width="100%"
                    cellpadding="0"
                    cellspacing="0"
                    style="
                      font-family: 'Roboto', sans-serif;
                      box-sizing: border-box;
                      font-size: 14px;
                      margin: 0;
                    "
                  >
                    <tbody>
                      <tr
                        style="
                          font-family: 'Roboto', sans-serif;
                          box-sizing: border-box;
                          font-size: 14px;
                          margin: 0;
                        "
                      >
                        <td
                          class="content-block"
                          style="
                            font-family: 'Roboto', sans-serif;
                            box-sizing: border-box;
                            font-size: 14px;
                            vertical-align: top;
                            margin: 0;
                            padding: 0 0 20px;
                          "
                          valign="top"
                        >
                          <div style="margin-bottom: 15px">
                            <img
                              src="<%= config.siteLogo %>"
                              alt="logo"
                              height="23"
                            />
                          </div>
                        </td>
                      </tr>

                      <tr
                        style="
                          font-family: 'Roboto', sans-serif;
                          box-sizing: border-box;
                          font-size: 14px;
                          margin: 0;
                        "
                      >
                        <td
                          class="content-block"
                          style="
                            font-family: 'Roboto', sans-serif;
                            color: #878a99;
                            box-sizing: border-box;
                            line-height: 1.5;
                            font-size: 15px;
                            vertical-align: top;
                            margin: 0;
                            padding: 0 0 10px;
                          "
                          valign="top"
                        >
                          <p style="margin-bottom: 1rem">Dear, <%= name %></p>
                          <p style="margin-bottom: 0.5rem">
                            We are thrilled to welcome you to <%= siteName %>,
                            your gateway to a world of digital collectibles and
                            unique NFT experiences! ðŸš€
                          </p>
                          <p style="margin-bottom: 0.5rem">
                            Thank you for choosing <%= siteName %> as your
                            platform to explore, buy, and sell NFTs. We're
                            excited to have you join our vibrant community of
                            artists, collectors, and enthusiasts.
                          </p>
                          <p style="margin-bottom: 0.5rem; font-weight: bold">
                            What's waiting for you on <%= siteName %>:
                          </p>
                          <ol>
                            <li>
                              <p>
                                <span style="font-weight: bold"
                                  >Explore Collections:
                                </span>
                                Dive into a vast array of NFT collections from
                                artists and creators worldwide. Whether you're
                                into digital art, music, virtual real estate, or
                                anything in between, you're bound to discover
                                something you love.
                              </p>
                            </li>
                            <li>
                              <p>
                                <span style="font-weight: bold"
                                  >Create and Mint NFTs:
                                </span>
                                Unleash your creative side by minting your own
                                NFTs. <%= siteName %> offers tools and resources
                                to help you tokenize your digital creations and
                                share them with the world.
                              </p>
                            </li>
                            <li>
                              <p>
                                <span style="font-weight: bold"
                                  >Auction and Trade:
                                </span>
                                Buy, sell, and trade NFTs in a secure and
                                decentralized marketplace. Our platform makes it
                                easy to connect with other collectors and make
                                unique transactions.
                              </p>
                            </li>
                            <li>
                              <p>
                                <span style="font-weight: bold"
                                  >Community Engagement:
                                </span>
                                Join discussions, forums, and events within the
                                <%= siteName %> community. Connect with
                                like-minded individuals, share your passion, and
                                gain insights from others in the NFT space.
                              </p>
                            </li>
                          </ol>
                          <p style="margin-bottom: 0.5rem; font-weight: bold">
                            Getting Started:
                          </p>
                          <ol>
                            <li>
                              <p>
                                <span style="font-weight: bold"
                                  >Complete Your Profile:
                                </span>
                                To get the most out of your <%= siteName %>
                                experience, make sure to set up your profile
                                with a profile picture and bio.
                              </p>
                            </li>
                            <li>
                              <p>
                                <span style="font-weight: bold"
                                  >Wallet Integration:
                                </span>
                                Unleash your creative side by minting your own
                                NFTs. <%= siteName %> offers tools and resources
                                to help you tokenize your digital creations and
                                share them with the world.
                              </p>
                            </li>
                            <li>
                              <p>
                                <span style="font-weight: bold"
                                  >Discover and Collect:
                                </span>
                                Start exploring our marketplace and find NFTs
                                that resonate with you. The possibilities are
                                endless!
                              </p>
                            </li>
                          </ol>
                          <p style="margin-bottom: 0.5rem">
                            Should you have any questions or need assistance,
                            our support team is here to help. Don't hesitate to
                            reach out to <%= config.siteEmail %> for any
                            inquiries.
                          </p>
                          <p style="margin-bottom: 0.5rem">
                            Thank you once again for choosing <%= siteName %>.
                            We look forward to seeing your NFT journey unfold on
                            our platform. Together, we'll shape the future of
                            digital ownership and creativity.
                          </p>
                          <p style="margin-bottom: 0.5rem">
                            Welcome to the <%= siteName %> family! ðŸŽ‰
                          </p>
                          <p style="margin-bottom: 0.5rem">Warm regards,</p>
                        </td>
                      </tr>

                      <tr
                        style="
                          font-family: 'Roboto', sans-serif;
                          box-sizing: border-box;
                          font-size: 14px;
                          margin: 0;
                        "
                      >
                        <td
                          class="content-block"
                          itemprop="handler"
                          itemscope=""
                          itemtype="http://schema.org/HttpActionHandler"
                          style="
                            font-family: 'Roboto', sans-serif;
                            box-sizing: border-box;
                            font-size: 14px;
                            vertical-align: top;
                            margin: 0;
                            padding: 0 0 24px;
                          "
                          valign="top"
                        >
                          <a
                            href="<%= btnLink %>"
                            data-link-replace="<%= btnLink %>"
                            itemprop="url"
                            style="
                              font-family: Roboto, sans-serif;
                              box-sizing: border-box;
                              font-size: 0.8125rem;
                              font-weight: 400;
                              color: rgb(255, 255, 255);
                              text-decoration: none;
                              text-align: center;
                              cursor: pointer;
                              display: inline-block;
                              border-radius: 0.25rem;
                              text-transform: capitalize;
                              background: rgb(10, 179, 156);
                              margin: 0px;
                              border-color: rgb(10, 179, 156);
                              border-style: solid;
                              border-width: 1px;
                              padding: 0.5rem 0.9rem;
                            "
                            onmouseover="this.style.background='#099885'"
                            onmouseout="this.style.background='#0ab39c'"
                            >Get Started</a
                          >
                        </td>
                      </tr>

                      <tr
                        style="
                          font-family: 'Roboto', sans-serif;
                          box-sizing: border-box;
                          font-size: 14px;
                          margin: 0;
                          border-top: 1px solid #e9ebec;
                        "
                      >
                        <td
                          class="content-block"
                          style="
                            font-family: 'Roboto', sans-serif;
                            box-sizing: border-box;
                            font-size: 14px;
                            vertical-align: top;
                            margin: 0;
                            padding: 0;
                            padding-top: 15px;
                          "
                          valign="top"
                        >
                          <div style="display: flex; align-items: center">
                            <img
                              src="<%= config.siteLogo %>"
                              alt="profile"
                              height="35"
                              width="35"
                              style="border-radius: 50px"
                            />
                            <div style="margin-left: 8px">
                              <span style="font-weight: 8"
                                ><%= siteName %> Bot</span
                              >
                              <p
                                style="
                                  font-size: 13px;
                                  margin-bottom: 0px;
                                  margin-top: 3px;
                                  color: #878a99;
                                "
                              >
                                Noreply
                              </p>
                            </div>
                          </div>
                        </td>
                      </tr>
                    </tbody>
                  </table>
                </td>
              </tr>
            </tbody>
          </table>
        </div>
      </td>
    </tr>
  </tbody>
</table>
